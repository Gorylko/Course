@using Shop.Web.Models
@model ImageViewModel

<table>
    <tr>
        <th>айдиха</th>
        <th>Картинка</th>
    </tr>

    @foreach (var image in @Model.Images)
    {
        <tr>
            <td>
                @image.Id
            </td>
            <td>
                @Html.Raw("<img style='width:80px; height:60px;' src=\"data:image/jpeg;base64,"
                                    + Convert.ToBase64String(image.Data) + "\" />")
            </td>
        </tr>
    }
</table>